{
  "name": "pure-data",
  "version": "0.0.1",
  "description": "Pure Data Design Pattern Test Application",
  "scripts": {
    "test": "jest",
    "start": "webpack-dev-server",
    "build": "tsc -p ."
  },
  "author": "Deepak Patil",
  "license": "MIT",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "devDependencies": {
    "@types/enzyme": "^3.1.10",
    "@types/jest": "^23.1.0",
    "@types/prop-types": "^15.5.3",
    "@types/react": "^16.3.16",
    "@types/react-router": "^4.0.26",
    "@types/react-router-dom": "^4.2.7",
    "awesome-typescript-loader": "^5.0.0",
    "chai": "^4.1.2",
    "coveralls": "^3.0.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.1.0",
    "mocha": "^5.2.0",
    "react-dom": "^16.4.1",
    "react-hot-loader": "^4.2.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "source-map-loader": "^0.2.3",
    "ts-jest": "^22.4.6",
    "typescript": "^2.9.1",
    "webpack": "^4.10.2",
    "webpack-cli": "^3.0.1",
    "webpack-dev-server": "^3.1.4"
  },
  "dependencies": {
    "react": "^16.4.0",
    "prop-types": "^15.6.1"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "useBabelrc": true
      }
    },
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{ts}"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "setupTestFrameworkScriptFile": "./test/setupTest.ts"
  }
}
